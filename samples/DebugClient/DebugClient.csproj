<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="appsettings.json" />
    <None Remove="jsonconfig\jtt809-protocol-datamappings.json" />
    <None Remove="jsonconfig\jtt809-protocol-internalentitysmappings.json" />
    <None Remove="jsonconfig\jtt809-protocol-structures.json" />
    <None Remove="jsonconfig\jtt809-protocol.json" />
